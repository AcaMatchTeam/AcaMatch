<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.studybridge.grade.GradeMapper">
    <insert id="insGrade" useGeneratedKeys="true" keyProperty="gradeId">
        INSERT INTO grade
        SET subject_id = #{subjectId},
        grade = #{grade},
        pass = #{pass}
    </insert>
</mapper>